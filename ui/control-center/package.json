{
  "name": "@atlasmesh/control-center",
  "version": "2.0.0",
  "description": "AtlasMesh Fleet OS Control Center - AV Fleet Management Interface",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "a11y": "axe-core --dir dist --include=\"**/*.html\"",
    "validate": "npm run type-check && npm run lint && npm run format:check && npm run test:coverage"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "react-query": "^3.39.3",
    "react-hook-form": "^7.43.1",
    "react-i18next": "^12.1.5",
    "react-helmet-async": "^1.3.0",
    "react-hot-toast": "^2.4.0",
    "react-error-boundary": "^4.0.11",
    "@reduxjs/toolkit": "^1.9.3",
    "react-redux": "^8.0.5",
    "redux-persist": "^6.0.0",
    "framer-motion": "^10.0.1",
    "@heroicons/react": "^2.0.16",
    "clsx": "^1.2.1",
    "tailwind-merge": "^1.10.0",
    "date-fns": "^2.29.3",
    "recharts": "^2.5.0",
    "mapbox-gl": "^2.13.0",
    "react-map-gl": "^7.0.21",
    "@mapbox/mapbox-gl-draw": "^1.4.0",
    "i18next": "^22.4.10",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.1.1",
    "zod": "^3.20.6",
    "@hookform/resolvers": "^2.9.11",
    "socket.io-client": "^4.6.1",
    "webrtc-adapter": "^8.2.3"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/mapbox-gl": "^2.7.10",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-react": "^3.1.0",
    "vite": "^4.1.4",
    "typescript": "^4.9.5",
    "tailwindcss": "^3.2.7",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-import": "^2.27.5",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.4",
    "vitest": "^0.29.2",
    "@vitest/ui": "^0.29.2",
    "@vitest/coverage-c8": "^0.29.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "jsdom": "^21.1.0",
    "msw": "^1.1.0",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-a11y": "^6.5.16",
    "@storybook/builder-vite": "^0.4.2",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@axe-core/cli": "^4.6.0",
    "chromatic": "^6.17.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}