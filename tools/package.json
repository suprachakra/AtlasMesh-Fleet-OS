{
  "name": "@atlasmesh/fleet-os-tools",
  "version": "1.0.0",
  "description": "Development tools for AtlasMesh Fleet OS",
  "private": true,
  "scripts": {
    "generate-go-sdk": "node sdk-generator/generate-go-sdk.js",
    "generate-typescript-sdk": "node sdk-generator/generate-typescript-sdk.js",
    "generate-python-sdk": "node sdk-generator/generate-python-sdk.js",
    "generate-all-sdks": "npm run generate-go-sdk && npm run generate-typescript-sdk && npm run generate-python-sdk",
    "validate-contracts": "node contract-testing/contract-validator.js",
    "validate-contracts-staging": "node contract-testing/contract-validator.js staging",
    "validate-contracts-prod": "node contract-testing/contract-validator.js production",
    "version-create": "node api-versioning/version-manager.js create",
    "version-deprecate": "node api-versioning/version-manager.js deprecate",
    "version-status": "node api-versioning/version-manager.js status",
    "version-list": "node api-versioning/version-manager.js list",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "axios": "^1.6.0",
    "js-yaml": "^4.1.0",
    "semver": "^7.5.4",
    "@apidevtools/swagger-parser": "^10.1.0",
    "openapi-generator-cli": "^2.7.0",
    "commander": "^11.1.0",
    "chalk": "^4.1.2",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1",
    "boxen": "^5.1.2"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "@types/js-yaml": "^4.0.8",
    "@types/semver": "^7.5.6",
    "@types/inquirer": "^9.0.7"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "atlasmesh",
    "fleet-os",
    "api-contracts",
    "sdk-generation",
    "openapi",
    "development-tools"
  ],
  "author": "AtlasMesh API Team",
  "license": "PROPRIETARY"
}
